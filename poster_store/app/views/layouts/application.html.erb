			<%= csrf_meta_tags %>
		<style>
			p.sans{font-family:"Comic Sans MS", cursive, sans-serif;}
		</style>
	</head>
<body>
	<div id="container">
		<div id="header">
			<h1>
				<%= @page_title || "WorldInMyLens" %>
